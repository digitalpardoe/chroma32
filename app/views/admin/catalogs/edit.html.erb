<h2>Editing Catalog</h2>

<% form_for(@catalog, :url => { :action => 'update' }) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
<% end %>

<ul class="links">
  <li><%= link_to 'Show', admin_catalog_path(@catalog) %></li>
  <li><%= link_to 'Back', admin_catalogs_path %></li>
</ul>

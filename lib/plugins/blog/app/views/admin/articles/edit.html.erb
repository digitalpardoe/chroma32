<h2>Editing Article</h2>

<% form_for(@article, :url => { :action => 'update' }) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
<% end %>

<ul class="links">
  <li><%= link_to 'Show', admin_article_path(@article) %></li>
  <li><%= link_to 'Back', admin_articles_path %></li>
</ul>
